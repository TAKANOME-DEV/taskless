{
  "name": "@taskless/client",
  "version": "0.0.2-rev1",
  "description": "Client for Taskless.io",
  "author": {
    "name": "Taskless.io",
    "email": "oss@taskless.io",
    "url": "https://taskless.io"
  },
  "repository": "taskless/taskless",
  "homepage": "https://github.com/taskless/taskless/tree/main/packages/taskless",
  "license": "MIT",
  "files": [
    "dist",
    "*.d.ts",
    "*.cjs",
    "*.js",
    "*.mjs",
    "!jest.config.js",
    "README.md"
  ],
  "main": "dist/cjs/src/index.js",
  "module": "dist/esm/src/index.js",
  "engines": {
    "node": "12 || 14 || 15 || 16 || 17"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "dev": "yarn watch",
    "codegen": "graphql-codegen",
    "build": "run-s build:tsc:esm build:tsc:cjs",
    "build:tsc:esm": "tsc --module es2015 --outDir dist/esm",
    "build:tsc:cjs": "tsc --outDir dist/cjs",
    "watch": "run-p watch:tsc:esm watch:tsc:cjs",
    "watch:tsc:esm": "tsc -w --module es2015 --outDir dist/esm",
    "watch:tsc:cjs": "tsc -w --outDir dist/cjs",
    "prepack": "run-s build",
    "lint": "eslint",
    "release": "release-it"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "p-retry": "^5.0.0",
    "phin": "^3.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/time": "^3.1.1",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-generic-sdk": "^2.3.4",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@types/babel__core": "^7",
    "@types/deepmerge": "^2.2.0",
    "@types/eslint": "^8",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/uuid": "^8",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.12.6",
    "next": "^12.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsdef": "^0.0.14",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "next": "^10.0.8 || ^11.0 || ^12.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "np": {
    "message": "chore: Increments package.json to %s"
  }
}
